{
  "manifest_version": 2,
  "name": "Github.com Source Viewer",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "permissions" : ["tabs","https://github.com/"],
  "content_scripts":[{
    "matches":["https://github.com/*"],
    "js":["jquery-2.0.0.min.js", "content_script.js"]
  }]
}
